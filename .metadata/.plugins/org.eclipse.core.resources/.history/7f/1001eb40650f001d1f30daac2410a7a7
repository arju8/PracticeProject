package com.practice;

public class IntrvwProgram {
	public static void main(String[] args) {
		String s="iyefgyigfeyfgy10wrbhbr100bvhe4";
		
		s = s.replaceAll("\\D", " ");
		System.out.println(s);
		
		s = s.trim();
		System.out.println(s);
		
		s=s.replaceAll(" +", " ");
		System.out.println(s);
		
		
				
	}

}
